{
  "name": "lara-pie/larapie",
  "description": "Generating API documentation from the Laravel routes",
  "minimum-stability": "stable",
  "license": "proprietary",
  "type": "library",
  "authors": [
    {
      "name": "lara-pie",
      "email": "larapie.package@gmail.com"
    }
  ],
  "require": {
    "php": "^8.0"
  },
  "require-dev": {
    "orchestra/testbench": "^6.19"
  },
  "autoload": {
    "psr-4": {
      "Larapie\\Larapie\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Larapie\\Larapie\\LarapieServiceProvider"
      ],
      "aliases": {
        "Larapie": "Larapie\\Larapie\\Facades\\Larapie"
      }
    }
  }
}
